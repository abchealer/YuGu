#include <bits/stdc++.h>
using namespace std;

const int N = 32;

int A[N][N][N];
int w,x,h,q;
int a,b,c,d,e,f;
int cnt;

void solve() {
	for(int i=a;i<=d;i++) {
		for(int j=b;j<=e;j++) {
			for(int k=c;k<=f;k++) {
				A[i][j][k] = 1;
			}
		}
	}
}

int main() {
	scanf("%d %d %d %d",&w,&x,&h,&q);
	for(int i=1;i<=q;i++) {
		scanf("%d %d %d %d %d %d",&a,&b,&c,&d,&e,&f);
		solve();
	}
	for(int i=1;i<=w;i++) {
		for(int j=1;j<=x;j++) {
			for(int k=1;k<=h;k++) {
				if(A[i][j][k] == 0)
					++cnt;
			}
		}
	}
	printf("%d\n",cnt);
	return 0;
}
