#include <iostream>
using namespace std;
char ch;
int main(){
    if(cin>>ch){
        if(ch>='a'&&ch<='z')
            ch+='A'-'a';
        cout<<ch;
        main();
    }
    
    
    
    return 0;
}
/*
第二种做法：（使用函数） 
#include <iostream>
using namespace std;
int main(){
	
	string s;
	cin>>s;
	for(int i=0;i<s.size();i++){
		if(!isupper(s[i])){
			s[i]=toupper(s[i]);
		} 
	}
	
	cout<<s;
	
	return 0;
}
*/
